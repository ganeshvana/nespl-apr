</template>


     <template id="solar_report_report">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="oi_novergy.report_solar_report_template" t-lang="doc.name.lang"/>
                </t>
            </t>
        </template>
    </data>
</odoo>  