             <tr style="solid;border-width:1px;width:100%">

                    <td style="border-style: solid;border-width:1px;" width="10%">O</td>
                    <td style="border-style: solid;border-width:1px;text-align:center;" width="10%">N X J</td>
                    <td style="border-style: solid;border-width:1px;text-align:center;" width="40%">Energy with Multiplication factor (KWH):</td>
                    <td style="border-style: solid;border-width:1px;text-align:center;" width="40%"><span t-field="doc.multiplication_factor_energy"/></td>
                        


             </tr>
         </table>
 
        
          <table   style="border-style: solid;border-width:1px;border-top:hidden" width="100%">

             <tr style="solid;border-width:1px;width:100%">

                    <td style="border-style: solid;border-width:1px;" width="10%">P</td>
                    <td style="border-style: solid;border-width:1px;text-align:center;" width="10%">O</td>
                    <td style="border-style: solid;border-width:1px;text-align:center;" width="40%">TOTAL SOLAR ENERGY FOR MONTH (in Figures):-</td>
                    <td style="border-style: solid;border-width:1px;text-align:center;" width="40%"><span t-field="doc.total_solar_energy"/></td>
                        


             </tr>
         </table>

