<odoo>
    <data>
        <template id="sale_report_external_layout">
            <div t-attf-class="header" t-att-style="report_header_style">
                
               
            </div>
            <div t-attf-class="article o_report_layout_standard " t-att-data-oe-model="o and o._name" t-att-data-oe-id="o and o.id" t-att-data-oe-lang="o and o.env.context.get('lang')">
                
                <t t-raw="0"/>
            </div>
            <div t-attf-class="footer o_standard_footer ">
                <table width="100%" style="border:none;">
                    <tr>
                        <td><center>Page: <span class="page"/> / <span class="topage"/></center></td>
                    </tr>
                </table>
            </div>
        </template>


        <template id="report_solar_report_template">
            <t t-call="oi_novergy.sale_report_external_layout">
                <!-- <t t-call="web.external_layout"> -->

                <t t-set="doc" t-value="doc.with_context(lang=doc.name.lang)"/>
                <div class="page">
                    <style>
                    
                    table { page-break-inside:auto }
                    tr    { page-break-inside:avoid; page-break-after:auto }
                        thead { display:table-header-group }

                    td{
                        padding-left:2px;
                        padding-right:2px;
                    }

                    </style>

            
            <table   style="border-style: solid;border-width:1px;border:none" width="100%">

             <tr style="solid;border-width:1px;width:100%;border:none">

                    <td style="border-style: solid;border-width:1px;text-align:right;border:none" width="70%"><strong><span t-field="doc.company_id"/></strong></td>
                    <td style="border-style: solid;border-width:1px;text-align:right:border:none;border-top:hidden;border-bottom:hidden;border-right:hidden;border-left:hidden" width="30%"><img t-att-src="image_data_uri(doc.company_id.logo)" style="max-height: 4cm; max-width: 4cm; border:none" align="right"/></td>


             </tr>
            </table>
                    

            <table   style="border-style: solid;border-width:1px;" width="100%">

             <tr style="solid;border-width:1px;width:100%">

                    <td style="border-style: solid;border-width:1px;text-align:center" width="100%"><strong>SOLAR ENERGY JOINT READING</strong></td>
                   

