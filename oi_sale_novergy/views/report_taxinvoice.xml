							<td style="border-style: solid;border-width:1px;text-align:center;border-top:hidden" width="5%">
								<strong>Qty.</strong>
							</td>
							<td style="border-style: solid;border-width:1px;text-align:center;border-top:hidden" width="10%">
								<strong>Unit</strong>
							</td>
							
							<td style="border-style: solid;border-width:1px;text-align:center;border-top:hidden" width="10%">
